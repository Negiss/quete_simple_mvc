{% extends 'layout.html.twig' %}

{% block title %}MVC 4{% endblock %}
{% block content %}
<section>
    <h1>Items</h1>
    <ul>
        {% for item in items %}
            <li>{{ item.title }}<a href="/items/edit/{{ item.id }}">Edit </a><a href="/items/delete/{{ item.id }}"> Delete</a></li>
        {% endfor %}
    </ul>

    <a href="items/add">Add</a>
</section>
{% endblock %}

{% block javascript %}{% endblock %}